	xmlns:xsi="https://jmapper-framework.googlecode.com/svn"
	xsi:noNamespaceSchemaLocation="https://jmapper-framework.googlecode.com/svn/jmapper.xsd">
	<class
		name="com.googlecode.jmapper.integrationtest.configurations.bean.JMapAccDest2">
		<global>
			<value name="sField2" get="getSSField2" />
			<attributes>
				<attribute name="dField2" get="getDestField2" set="setDestField2"/>
			</attributes>
			
			<!-- provare anche con excluded 
				<excluded>
					<attribute name="dField1" />
				</excluded>
			-->
		</global>
		<attribute name="dField1" get="getDestField1" set="setDestField1">
			<value name="sField1" get="NON_LETTO"/>
		</attribute>
		<attribute name="dField2" get="getDestField2" set="setDestField2">
			<value name="sField2" get="getSSField2"/>
		</attribute>
	</class>
	<class
		name="com.googlecode.jmapper.integrationtest.configurations.bean.JMapAccSrc2">
		<attribute name="sField1" get="getsField1" />
	</class>
</jmapper>